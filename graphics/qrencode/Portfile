PortSystem          1.0

name                qrencode
version             1.0.2
revision            0
categories          graphics
maintainers         nomaintainer@macports.org
description         encode/decode QR code.
long_description    Libqrencode is a C library for encoding data in a QR Code symbol, a kind of 2D symbology that can be scanned by handy terminals such as a mobile phone with CCD. The capacity of QR Code is up to 7000 digits or 4000 characters, and is highly robustness.
homepage            http://megaui.net/fukuchi/works/qrencode/index.html
platforms           darwin freebsd

distfiles           ${name}-${version}.tar.gz:qrencode
use_autoconf        yes

set serverList {
  http://megaui.net/fukuchi/works/qrencode
}

foreach server ${serverList} {
    lappend master_sites ${server}/:qrencode
}

checksums md5 add273fb140bc73bb5f52fcda00614a4
